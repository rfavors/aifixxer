<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <!-- Background rounded rectangle with teal color -->
  <rect x="2" y="2" width="28" height="28" rx="4" ry="4" fill="#0f4c5c" stroke="none"/>
  
  <!-- Robot head circle -->
  <circle cx="16" cy="12" r="3.5" fill="#ffffff"/>
  
  <!-- Robot eyes -->
  <circle cx="15" cy="11.5" r="0.6" fill="#0f4c5c"/>
  <circle cx="17" cy="11.5" r="0.6" fill="#0f4c5c"/>
  
  <!-- Robot antenna/connectors -->
  <circle cx="13" cy="7" r="0.4" fill="#ffffff"/>
  <circle cx="19" cy="7" r="0.4" fill="#ffffff"/>
  <line x1="13.5" y1="9" x2="13" y2="7.4" stroke="#ffffff" stroke-width="0.3"/>
  <line x1="18.5" y1="9" x2="19" y2="7.4" stroke="#ffffff" stroke-width="0.3"/>
  
  <!-- Robot body/chest -->
  <rect x="14" y="15" width="4" height="3" rx="0.5" ry="0.5" fill="#ffffff"/>
  
  <!-- Wrench tools crossed -->
  <g transform="translate(12, 20) rotate(-30)">
    <rect x="0" y="0" width="3" height="0.5" rx="0.25" ry="0.25" fill="#ffffff"/>
    <rect x="2.5" y="-0.3" width="1" height="1.1" rx="0.2" ry="0.2" fill="#ffffff"/>
  </g>
  
  <g transform="translate(18, 20) rotate(30)">
    <rect x="0" y="0" width="3" height="0.5" rx="0.25" ry="0.25" fill="#ffffff"/>
    <rect x="2.5" y="-0.3" width="1" height="1.1" rx="0.2" ry="0.2" fill="#ffffff"/>
  </g>
  
  <!-- Circuit pattern decorations -->
  <circle cx="7" cy="9" r="0.3" fill="#ffffff"/>
  <circle cx="25" cy="9" r="0.3" fill="#ffffff"/>
  <circle cx="7" cy="23" r="0.3" fill="#ffffff"/>
  <circle cx="25" cy="23" r="0.3" fill="#ffffff"/>
</svg>