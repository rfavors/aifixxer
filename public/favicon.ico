<svg width="32" height="32" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg">
  <!-- Background rounded rectangle -->
  <rect x="2" y="2" width="28" height="28" rx="4" ry="4" fill="#0f4c5c" stroke="none"/>
  
  <!-- Robot head circle -->
  <circle cx="16" cy="13" r="4" fill="#ffffff"/>
  
  <!-- Robot eyes -->
  <circle cx="14.5" cy="12" r="0.8" fill="#0f4c5c"/>
  <circle cx="17.5" cy="12" r="0.8" fill="#0f4c5c"/>
  
  <!-- Robot antenna/connectors -->
  <circle cx="12" cy="8" r="0.5" fill="#ffffff"/>
  <circle cx="20" cy="8" r="0.5" fill="#ffffff"/>
  <line x1="13" y1="10" x2="12" y2="8.5" stroke="#ffffff" stroke-width="0.5"/>
  <line x1="19" y1="10" x2="20" y2="8.5" stroke="#ffffff" stroke-width="0.5"/>
  
