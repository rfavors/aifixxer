  <!-- Wrench tools -->
  <g transform="translate(10, 22) rotate(-15)">
    <rect x="0" y="0" width="4" height="0.8" rx="0.4" ry="0.4" fill="#ffffff"/>
    <rect x="3" y="-0.5" width="1.5" height="1.8" rx="0.2" ry="0.2" fill="#ffffff"/>
  </g>
  
  <g transform="translate(18, 22) rotate(15)">
    <rect x="0" y="0" width="4" height="0.8" rx="0.4" ry="0.4" fill="#ffffff"/>
    <rect x="3" y="-0.5" width="1.5" height="1.8" rx="0.2" ry="0.2" fill="#ffffff"/>
  </g>
  
  <!-- Circuit pattern decorations -->
  <circle cx="7" cy="10" r="0.4" fill="#ffffff"/>
  <circle cx="25" cy="10" r="0.4" fill="#ffffff"/>
  <circle cx="7" cy="22" r="0.4" fill="#ffffff"/>
  <circle cx="25" cy="22" r="0.4" fill="#ffffff"/>
</svg>